html, body {
    height: 100%;
}

#map {
    width: 100%;
    height: 100%;
    min-height: 100%;
    display: block;
}

a:focus {
	outline-width: 0;
}

div:focus {
	outline-width: 0;	
}

#video-panel {
	position: fixed;
	width:100%;
	height: auto;
	top:40px;
	bottom: 0;
	background-color: black;
}

@media (max-width: 768px) {
	#video-panel {
		position: fixed;
		width:100%;
		overflow: scroll;
	}
}

.small-video-display {
	float: left;
	width: 50%;
	height: 50%;
}

@media (max-width: 768px) {
	.small-video-display {
		width: 100%;
		height: 100%;
	}
}

.correct-top {
	height: 75px;
}

.column-bottom-padding {
	padding-bottom: 0;
}

@media (max-width: 992px) {
	.column-bottom-padding {
		padding-bottom: 10px;
	}
}

.full-bottom-padding {
	padding-bottom: 10px;
}

.map_marker {
	padding: 0 !important;
	font-family: 'Glyphicons Halflings';
  font-style: normal;
  font-weight: normal;
  font-size: 19px !important;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  height: auto !important;
}

.map_marker:before {
	content: "\e046";
}
